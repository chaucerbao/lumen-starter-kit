{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": ["framework", "laravel", "lumen"],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/lumen-framework": "5.0.*",
    "vlucas/phpdotenv": "~1.0",
    "illuminate/mail": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "league/factory-muffin": "~2.1",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
